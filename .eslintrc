{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jquery": true,
        "mocha": true
    },
    "globals": {
        "io": false,
        "moment": false,
        "Mustache": true
    },
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ]
        
    }
}